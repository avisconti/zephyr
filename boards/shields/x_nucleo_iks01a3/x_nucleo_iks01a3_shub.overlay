&arduino_i2c {

	lis2dw12@19 {
		compatible = "st,lis2dw12";
		reg = <0x19>;
		irq-gpios = <&gpiob 0 0>;
		label = "LIS2DW12";
	};

	lsm6dso@6b {
		compatible = "st,lsm6dso";
		reg = <0x6b>;
		irq-gpios = <&gpiob 5 0>;
		label = "LSM6DSO";
	};
};
